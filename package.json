{
  "name": "Scheduler",
  "version": "1.0.0",
  "description": "Scheduler for gyms",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime",
      "react-hot-loader/babel"
    ]
  },
  "scripts": {
    "webserver": "node start.js",
    "build": "node_modules/.bin/webpack",
    "dev": "node_modules/.bin/webpack-dev-server",
    "start": "npm run dev",
    "clean": "rm -rf node_modules app/build app/dist",
    "test": "node_modules/.bin/mocha \"test/*@(.js|.jsx)\"",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint . --ext .js",
    "deploy": "NODE_ENV=production webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alicjakujawa/scheduler.git"
  },
  "keywords": [
    "starter",
    "koa",
    "redux",
    "react"
  ],
  "author": "Alicja Kujawa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alicjakujawa/scheduler/issues"
  },
  "homepage": "https://github.com/alicjakujawa/scheduler#readme",
  "dependencies": {
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.18.0",
    "bookshelf": "^0.10.2",
    "css-loader": "^0.25.0",
    "html-webpack-plugin": "^2.24.0",
    "immutable": "^3.8.1",
    "knex": "^0.12.6",
    "koa": "^2.0.0",
    "koa-cors": "0.0.16",
    "koa-router": "^7.0.1",
    "node-sass": "^3.10.1",
    "pg": "^6.1.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "eslint": "^3.9.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "jsdom": "^9.8.3",
    "mocha": "^3.1.2",
    "react-addons-test-utils": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.1.1",
    "webpack-dev-server": "^1.16.2"
  }
}
